<div class="container">
  <h2>Discount Panel</h2>
  <div class="panel panel-default">
    <div class="panel-body">
   <div class="demo carousel slide carousel_imgslide" data-ride="carousel">
        <p id="paraforimages carousel-inner carousel-items">
{{#each new_products}}

        {{#each this}}
           {{#if this.imagePath}}
        <a href="/productview/{{ this._id }}">
                    <img class="card" src="{{this.imagePath}}" onclick="/productview/{{ this._id }}"alt="..." height="250px"width="200px">
      </a>
      {{/if}}
        {{/each}}
{{/each}}
</p>

<a class="left carousel-control" id="move-left"role="button" width="50px" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left"></span>
  </a>
  <a class="right carousel-control" id="move-left" role="button" width="50px" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right"></span>
  </a>
</div> 

    </div>
  </div>
</div>

